{
  "npmScope": "nx-starter",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "shared-assets": {
      "tags": ["type:assets", "scope:global"]
    },
    "shared-directives": {
      "tags": ["type:ui", "scope:web"]
    },
    "shared-pipes": {
      "tags": ["type:ui", "scope:web"]
    },
    "shared-ui": {
      "tags": ["type:ui", "scope:web"]
    },
    "shared-utils": {
      "tags": ["type:utils", "scope:global"]
    },
    "web-client": {
      "tags": []
    },
    "web-client-e2e": {
      "tags": [],
      "implicitDependencies": ["web-client"]
    }
  }
}
